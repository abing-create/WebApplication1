
@using (Html.BeginForm("moreSearchStorage", "Home", FormMethod.Post, new { @id = "delete-form", role = "form" }))
{
    <div class="modal-dialog modal-dialog-centered" role="document">
        <div class="modal-content">
            <div class="modal-header">
                <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
                <h4 class="modal-title" id="myModalLabel">多重查询</h4>
            </div>
            <div class="modal-body">
                <div>
                    <table class="table-borderless">
                        <tr>
                            <td>
                                入库时间:
                            </td>
                            <td>
                                <input type="text" id="start_date" name="start_date" placeholder="yyyy-MM-dd HH:mm:ss" aria-label="yyyy-MM-dd HH:mm:ss" value="" />
                            </td>
                            <td>
                                <label>----></label>
                            </td>
                            <td>
                                <input type="text" id="end_date" name="end_date" placeholder="yyyy-MM-dd HH:mm:ss" aria-label="yyyy-MM-dd HH:mm:ss" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                入库员工:
                            </td>
                            <td>
                                <input type="text" id="U_name" name="U_name" value="" />
                            </td>
                            <td>
                                员工编号:
                            </td>
                            <td>
                                <input type="text" id="U_Id" name="U_Id" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                存储仓库:
                            </td>
                            <td>
                                <input type="text" id="W_name" name="W_name" value="" />
                            </td>
                            <td>
                                仓库编号:
                            </td>
                            <td>
                                <input type="text" id="Wa_Id" name="Wa_Id" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                仓库地址:
                            </td>
                            <td>
                                <input type="text" id="Wa_address" name="Wa_address" value="" />
                            </td>
                            <td>
                                商品名称:
                            </td>
                            <td>
                                <input type="text" id="Co_name" name="Co_name" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                商品编号:
                            </td>
                            <td>
                                <input type="text" id="Co_Id" name="Co_Id" value="" />
                            </td>
                            <td>
                                条码编号:
                            </td>
                            <td>
                                <input type="text" id="Co_bar_code" name="Co_bar_code" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                商品分类:
                            </td>
                            <td>
                                <input type="text" id="Co_type" name="Co_type" value="" />
                            </td>
                            <td>
                                商品规格:
                            </td>
                            <td>
                                <input type="text" id="Co_specification" name="Co_specification" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                商品单价:
                            </td>
                            <td>
                                <input type="text" id="Co_price" name="Co_price" value="" />
                            </td>
                            <td>
                                计量单位:
                            </td>
                            <td>
                                <input type="text" id="Co_unit" name="Co_unit" value="" />
                            </td>
                        </tr>

                        <tr>
                            <td>
                                商品重量:
                            </td>
                            <td>
                                <input type="text" id="Co_weight" name="Co_weight" value="" />
                            </td>
                            <td>
                                入库单号:
                            </td>
                            <td>
                                <input type="text" id="IO_Id" name="IO_Id" value="" />
                            </td>
                        </tr>

                    </table>
                </div>
            </div>
            <div class="modal-footer">
                <button type="button" class="btn btn-default" data-dismiss="modal" onclick="ResetUpdataForm();">取消</button>
                <input type="submit" class="btn btn-primary" name="BtnSubmit" value="查询" />
            </div>
        </div>
    </div>
}

