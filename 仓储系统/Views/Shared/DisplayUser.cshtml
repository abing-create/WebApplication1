@using 仓储系统.Models
@model User

<div class="modal-dialog modal-dialog-centered" role="document">
    <div class="modal-content">
        <div class="modal-header">
            <button type="button" class="close" data-dismiss="modal" aria-hidden="true">&times;</button>
            <h4 class="modal-title" id="myModalLabel">查看</h4>
        </div>
        <div class="modal-body">
            <div>
                <table class="table-borderless">
                    <tr>
                        <td>
                            员工编号:
                        </td>
                        <td>
                            <input type="text" id="U_Id" name="U_Id" readonly="readonly" value="@Model.U_Id" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            员工等级:
                        </td>
                        <td>
                            <input type="text" id="U_level" name="U_level" readonly="readonly" value="@Model.U_level" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            员工职务:
                        </td>
                        <td>
                            <input type="text" id="U_post" name="U_post" readonly="readonly" value="@Model.U_post" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            员工部门:
                        </td>
                        <td>
                            <input type="text" id="U_department" name="U_department" readonly="readonly" value="@Model.U_department" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            所属仓库:
                        </td>
                        <td>
                            <input type="text" id="U_point" name="U_point" readonly="readonly" value="@Model.U_point" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            员工姓名:
                        </td>
                        <td>
                            <input type="text" id="U_name" name="U_name" readonly="readonly" value="@Model.U_name" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            员工密码:
                        </td>
                        <td>
                            <input type="text" id="U_password" name="U_password" readonly="readonly" value="@Model.U_password" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            出生日期:
                        </td>
                        <td>
                            <input type="text" id="U_birthday" name="U_birthday" readonly="readonly" value="@Model.U_birthday.ToString("yyyy-MM-dd")" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            联系电话:
                        </td>
                        <td>
                            <input type="text" id="U_phone" name="U_phone" readonly="readonly" value="@Model.U_phone" />
                        </td>
                    </tr>
                    <tr>
                        <td>
                            性别:
                        </td>
                        <td>
                            <input type="text" id="U_sex" name="U_sex" readonly="readonly" value="@Model.U_sex" />
                        </td>
                    </tr>
                </table>
            </div>
        </div>
        <div class="modal-footer">
            <button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
        </div>
    </div>
</div>

